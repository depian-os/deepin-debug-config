msgid "Error: Invalid argument, --set and --get cannot be used together\n"
msgstr "Error: 參數無效，--set 和 --get 不能一起使用\n"

#, c-format
msgid "Error: %s missing parameter\n"
msgstr "Error: %s 缺失參數\n"

#, c-format
msgid "Try '%s --help' for more information.\n"
msgstr "嘗試使用'%s --help'獲取更多信息.\n"

#, c-format
msgid "Error: -l --level should be used with --set or --get\n"
msgstr "Error: -l --level需要與--set 或 --get一起使用\n"

#, c-format
msgid ""
"Error: Invalid argument for -c or --coredump which you should use with \"on"
"\" or \"off\"\n"
msgstr ""
"Error: -c 或 --coredump 的參數無效，應該與參數 \"on\" 或 \"off\" 一起使用\n"

#, c-format
msgid "Error: -c --coredump should be used with --set or --get\n"
msgstr "Error: -c --coredump需要與--set 或 --get一起使用\n"

#, c-format
msgid "Error: Invalid parameter, please check parameters\n"
msgstr "Error: 無效的參數，請檢查參數\n"

#, c-format
msgid "Error: Failed to get coredump state.\n"
msgstr "Error: 獲取配置核心轉儲狀態失敗\n"

#, c-format
msgid "Error: %s: Permission denied.\n"
msgstr "Error: %s: 權限被拒絕\n"

#, c-format
msgid ""
"Error: Invalid argument for -l --level, you should use --type with --level "
"whose parameter is \"on\" or \"off\"\n"
msgstr ""
"Error: -l --level 的參數無效，您應該將 --type 與 --level 一起使用，參數為 \"on"
"\" 或 \"off\"\n"

