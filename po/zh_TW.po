#, c-format
msgid "Usage: %s [options]\n"
msgstr "用法: %s [選項]\n"

#, c-format
msgid "options:\n"
msgstr "選項:\n"

#, c-format
msgid "\t-h --help:\tno arg,show help\n"
msgstr "t-h --help:\t沒有參數，顯示幫助\n"

#, c-format
msgid ""
"\t-s --set:\tno arg, means to configure debug mode, used together with --"
"coredump or --level\n"
msgstr ""
"\t-s --set:\t沒有參數，代表去配置調試模式，與--coredump 或 --level一起使用\n"

#, c-format
msgid ""
"\t-g --get:\tno arg, means to obtain debug mode information, used together "
"with --coredump or --level\n"
msgstr ""
"\t-g --get:\t沒有參數，代表去獲取調試模式信息，與--coredump 或 --level一起使"
"用\n"

#, c-format
msgid ""
"\t-c --coredump:\toptionally receive a parameter, depending on the --set and "
"--get options, which indicates switch coredump or get coredump status\n"
msgstr ""
"\t-c --coredump:\t可選地接收一個參數，具體取決於 --set 和 --get選項，表示切換"
"coredump或者獲取coredump狀態\n"

#, c-format
msgid ""
"\t-i --install-dbg:\trequire one arg, which means to install the debug "
"package of the specified pkg, example: -i systemd\n"
msgstr ""
"\t-i --install-dbg:\t需要一個參數，表示安裝指定包相關的調試包,示例: -i "
"systemd\n"

#, c-format
msgid ""
"\t-l --level:\toptionally receive a parameter, depending on the --set and --"
"get options, which indicates setting and getting the module log level and "
"coredump status\n"
msgstr ""
"\t-l --level:\t可選地接收一個參數，具體取決於 --set 和 --get獲取選項，表示設"
"置和獲取模塊日誌級別和核心轉儲狀態\n"

#, c-format
msgid ""
"\t-L --list-levels:\tno arg, but should be used with -m or --module, list "
"supported dbug levels for specified module \n"
msgstr ""
"\t-L --list-levels:\t沒有參數，但是必須與-m或--module一起使用，列出指定模組支"
"持的調試等級\n"

#, c-format
msgid ""
"\t-m --module:\trequire one arg, input the name of the module to be "
"configured, example: -m systemd\n"
msgstr ""
"\t-m --module:\t需要一個參數，輸入需要配置的模組的模組名，示例: -m systemd\n"

#, c-format
msgid ""
"\t-t --type:\trequire one arg, input the type of the module to be "
"configured, example: -t system\n"
msgstr ""
"\t-t --type:\t需要一個參數，輸入需要配置的模組的類型，示例: -t system\n"

#, c-format
msgid "Error: NOMEMORY:%m\n"
msgstr ""

